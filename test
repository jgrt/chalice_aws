import boto3
import time
import datetime
import json

s3 = boto3.resource("s3").Bucket("covid-dataset")
ts = time.time()
st = datetime.datetime.fromtimestamp(ts).strftime("%Y-%m-%d %H:%M:%S")
data = {"timestamp":st}
json.load_s3 = lambda f: json.load(s3.Object(key=ts).get()["Body"])
json.dump_s3 = lambda obj, f: s3.Object(key=f).put(Body=json.dumps(obj))

json.dump_s3(data, "output-1/"+str(ts)+".json")


